@model IEnumerable<fypPromolac.Models.user>

@{
    ViewBag.Title = "memberdetails";
}

    <h2 style="font-size: 34px;
    font-weight: 900;
    text-align: center">
        LOG INFORMATION
    </h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.userName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.m_Allowed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.detail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.m_sent)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.userName)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.m_Allowed)
    </td>

    <td>
        @foreach (var x in item.detail)
        {
            <span>@x.areaName</span><br />
        }
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.m_sent)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.userId })
        </td>
</tr>
}

</table>
